{
  "name": "marcbelletre/protect-the-children",
  "description": "Easily password protect the child pages/posts of a post/page that is password protected.",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Matt Miller"
    }
  ],
  "require": {
    "composer/installers": "^1.0|^2.0"
  },
  "autoload": {
    "psr-4": {
      "ProtectTheChildren\\": "lib/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
